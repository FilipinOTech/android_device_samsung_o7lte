# mount point   fstype      device                   device2
/boot           emmc        /dev/block/platform/7824900.sdhci/by-name/boot		flags=display="Kernel";backup=1
/recovery       emmc        /dev/block/platform/7824900.sdhci/by-name/recovery
/system         ext4        /dev/block/platform/7824900.sdhci/by-name/system
/cache          ext4        /dev/block/platform/7824900.sdhci/by-name/cache
/data           ext4        /dev/block/platform/7824900.sdhci/by-name/userdata	
/external_sd    vfat        /dev/block/mmcblk1p1     /dev/block/mmcblk1p1	flags=display="Micro SDcard";storage;wipeingui;removable
/usb_otg      	auto        /dev/block/sda1  /dev/block/sda              	flags=display="USB-OTG";storage;wipeingui;removable
